// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  name      String
  contact   String
  role      String
  comment   String?
  source    String?
  ip        String?
  userAgent String?

  @@map("leads")
} 